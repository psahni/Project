<% path_name = params[:controller].include?('admin') ? admin_game_path(game) : game_path(game) %>

<%unless game.image_file_name.blank? %>
	<%= link_to image_tag(game.image.url(:thumb), :class =>'game_image'), path_name %>
<% else %>
	<%= link_to image_tag("default_thumb.jpg", :class =>'game_image'), path_name %>
<% end %>
